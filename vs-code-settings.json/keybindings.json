//windows only
[
  {
    // press f1 once or twice to toggle between terminal and code
    "key": "f1",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "f1",
    "command": "workbench.action.terminal.focusNext",
    "when": "!terminalFocus"
  }
  {
    // use ctrl % to jump to closing tag in vs code vim, bec % simply dont work on html tags
    "key": "ctrl+shift+5",
    "command": "editor.emmet.action.matchTag"
  }
]
